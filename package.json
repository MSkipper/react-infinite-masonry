    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,json,scss,css,md}": ["prettier --write", "git add"]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@parcel/transformer-typescript-types": "^2.0.0-rc.0",
    "@parcel/validator-typescript": "^2.0.0-nightly.890",
    "@types/react": "^17.0.24",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.1",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.2.3",
    "parcel": "latest",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^16.0.0-0 || ^17.0.0"
