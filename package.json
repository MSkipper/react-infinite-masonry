{
  "name": "react-infinite-masonry",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "module": "dist/module.js",
  "scripts": {
    "dev": "parcel src/index.ts",
    "build": "parcel build --target main src/index.ts",
    "build-demo": "cd ./demo && yarn build ",
    "start-demo": "cd ./demo && yarn start ",
    "test": "jest --watch",
    "build:eslint": "eslint src/ --ext .ts --ext .tsx",
